
public import "std/concepts.znn";

template <T> requires comparable<T>
T min(mutable T a, mutable T b) {
  if (a < b)
    return move(a);
  else
    return move(b);
}

template <T> requires comparable<T>
T max(mutable T a, mutable T b) {
  if (a < b)
    return move(b);
  else
    return move(a);
}

int as_int(double a) {
  embed {
    return (int) a;
  }
}

double as_double(int a) {
  embed {
    return (double) a;
  }
}
