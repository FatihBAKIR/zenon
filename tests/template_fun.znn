template<T>
void copy(T mutable* out, mutable T in) {
  *out = move(in);
}

template<T>
variant V {
  T elem;
  void null;
};

template<T>
T get(mutable V<T> v, mutable T def) {
  switch (v) {
    case (mutable T elem) {
      return move(elem);
    }
    default {
      return move(def);
    }
  }
}

int main() {
  mutable V<int> v1 = V<int>::elem(7);
  mutable V<int> v2 = V<int>::null();
  mutable x = get(move(v1), 10) - get({v = move(v2), def = 5});
  copy(&x, x + 3);
  return x;
}
