// 14

template <T>
concept CanAdd {
  T(T*);
  T operator + (T a, T b);
};

template <T> requires CanAdd<T>
void increment(T* a, T b) {
  *a = *a + b;
}

struct A {
  int v;
};

A operator + (A a1, A a2) {
  return A(a1.v + a2.v);
}

int main() {
  int x = 2;
  x.increment(3);
  auto a = A(4);
  a.increment({b = A(5)});
  return x + a.v;
}
