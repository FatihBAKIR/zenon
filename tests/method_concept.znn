// 6

template<T, U>
concept CanConvert {
  U convert(T* t);
};

template <T>
struct S1 {
  T t;
};

template <T, U> requires CanConvert<U, T>
void set(mutable S1<T>* s, U u) {
  (*s).t = u.convert();
}

struct X {
  int a;
};

int convert(X* x) {
  return 1 + (*x).a;
}

int main() {
  mutable s1 = S1(1);
  s1.set(X(5));
  return s1.t;
}
