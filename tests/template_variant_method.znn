// 4

template <T>
variant V {
  T value;
  void null;
  T getValueOr(T a) {
    switch (*this) {
      case (T value) {
        return value;
      }
      case (null) {
        return a;
      }
    }
  }
};

int main() {
  auto v = V<int>::value(5);
  v = V<int>::null();
  return v.getValueOr(4);
}
